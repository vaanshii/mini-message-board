<!DOCTYPE html>
<html lang="en">
<%- include('head', {title: title}) %>

<body>
  <div class="wrapper">
    <%- include('navbar') %>
    <div class="message-board">
      <% messages.forEach( (message, index) => { %> <%- include('messageBox', {text:
				message.text, user: message.user, added: message.added, index: index}) %> <% }); %>
    </div>
  </div>
</body>

</html>